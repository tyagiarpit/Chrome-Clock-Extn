{
  "name": "Clock",
  "manifest_version": 2,
  "version": "1.0",
  "description": "Show Clock in Chrome Toolbar",
  "icons": { "16" : "images/icon.png",
 	    "32" : "images/32.png",
      "48" : "images/48.png",
      "64" : "images/64.png",
      "128" : "images/128.png",
      "256" : "images/256.png"},
  "homepage_url": "http://www.arpittyagi.com/",
	"background": {
    "page" : "background.html" },
	"browser_action": {
    "default_icon": "images/icon.png", 
    "default_title": "Clock"     
  },
  "options_page": "options.html",
  "permissions": [
     "storage"
  ]
}